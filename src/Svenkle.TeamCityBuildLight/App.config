<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <appSettings>
    <add key="ProjectFilter" value=".*"/>
    <add key="BuildFilter" value=".*"/>
    <add key="Frequency" value="15"/>
    <add key="Url" value=""/>
    <add key="Username" value=""/>
    <add key="Password" value=""/>
  </appSettings>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{HH:mm:ss} %-5level - %message%newline%exception" />
      </layout>
    </appender>
    <!--<appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="log.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="100MB" />
      <preserveLogFileNameExtension value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{HH:mm:ss} %-5level [%thread] %logger - %message%newline%exception" />
      </layout>
    </appender>-->
    <root>
      <level value="INFO" />
      <appender-ref ref="ConsoleAppender" />
      <!--<appender-ref ref="RollingLogFileAppender" />-->
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
</configuration>